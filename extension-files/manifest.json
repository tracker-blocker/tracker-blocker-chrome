{
    "name": "Tracker Blocker",
    "description": "Block domains from tracking you!",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "tabs", "webRequest", "webRequestBlocking", "<all_urls>"],
    "browser_action":{
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "/images/icons/tracker-blocker-logo_16.png",
        "32": "/images/icons/tracker-blocker-logo_32.png",
        "48": "/images/icons/tracker-blocker-logo_48.png",
        "128": "/images/icons/tracker-blocker-logo_128.png"
      }
}
